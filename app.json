{
  "expo": {
    "name": "Nayara AI",
    "slug": "nayara-ai-cancer-support",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nayara-ai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nayara.ai.cancer.support",
      "infoPlist": {
        "NSCameraUsageDescription": "This app may use the camera for profile photos",
        "NSPhotoLibraryUsageDescription": "This app may access photos for profile pictures"
      }
    },
    "android": {
      "package": "com.nayara.ai.cancer.support",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#F1C5D6"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "API_BASE_URL": "https://api.nayara.ai",
      "SENTRY_DSN": ""
    },
    "locales": {
      "he": "./assets/locales/he.json",
      "en": "./assets/locales/en.json"
    },
    "supportsRTL": true
  }
}